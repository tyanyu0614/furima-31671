<div class="main">
  <div class="inner">
    <div class="user__wrapper">
      <h2 class="page-heading">
        <%= "#{@user.nickname}さんの情報"%>
      </h2>
      <table class="table">
        <tbody>
          <tr>
            <th class="table__col1">ニックネーム</th>
            <td class="table__col2"><%= @user.nickname %></td>
          </tr>
          <tr>
            <th class="table__col1">Eメール</th>
            <td class="table__col2"><%= @user.email %></td>
          </tr>
          <tr>
            <th class="table__col1">姓</th>
            <td class="table__col2"><%= @user.last_name %></td>
          </tr>
          <tr>
            <th class="table__col1">名前</th>
            <td class="table__col2"><%= @user.first_name %></td>
          </tr>
          <tr>
            <th class="table__col1">生年月日</th>
            <td class="table__col2"><%= @user.birth_day %></td>
          </tr>
        </tbody>
      </table>
      <h2 class="page-heading">
        <%# <%= "#{@user.nickname}さんのプロトタイプ"%> 
      </h2>
      <div class="user__card">
        <%= link_to "トップページへ戻る", root_path%>
      
    </div>
  </div>
</div>